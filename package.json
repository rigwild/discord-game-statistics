{
  "name": "discord-game-statistics",
  "version": "1.0.0",
  "description": "Gather statistics on games played by people on Discord servers",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && tsc",

    "start": "cd dist/ && node index.js",
    "start:server": "cd dist/ && node index.js --no-bot",
    "start:bot": "cd dist/ && node index.js --no-server",

    "ts": "cd src/ && ts-node index.ts",
    "ts:server": "cd src/ && ts-node index.js --no-bot",
    "ts:bot": "cd src/ && ts-node index.js --no-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rigwild/discord-game-statistics.git"
  },
  "author": "rigwild",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rigwild/discord-game-statistics/issues"
  },
  "homepage": "https://github.com/rigwild/discord-game-statistics#readme",
  "dependencies": {
    "discord.js": "^11.4.2",
    "ejs": "^2.6.1",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "typescript": "^3.2.4"
  }
}
